import MobSDK from 'com.mob.MobSDK'
import { MobSDKInit, MobSDKsubmitPolicyGrantResult } from '../interface.uts';

export const mobSDKInit : MobSDKInit = function () : void {
	MobSDK.init(UTSAndroid.getAppContext())
}

export const mobSDKsubmitPolicyGrantResult : MobSDKsubmitPolicyGrantResult = function (granted : boolean) : void {
	MobSDK.submitPolicyGrantResult(true)
}
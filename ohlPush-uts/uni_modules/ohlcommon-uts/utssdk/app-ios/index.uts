import { 
	MobSDKInit,
	MobSDKsubmitPolicyGrantResult 
} from '../interface.uts';

export const mobSDKInit: MobSDKInit = function(key: string | null, secret: string | null) {
	UniOHLCommon.registerAppKey(key, secret);
}

export const mobSDKsubmitPolicyGrantResult: MobSDKsubmitPolicyGrantResult = function(granted: boolean) {
	UniOHLCommon.uploadPrivacyPermissionStatus(granted);
}
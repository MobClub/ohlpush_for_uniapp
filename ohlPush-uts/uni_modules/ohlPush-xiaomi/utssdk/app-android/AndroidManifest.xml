<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
	package="io.dcloud.uni_modules.ohlPushXiaomi">
	<application>

		<receiver android:name="com.ohaola.sdk.push.plugins.xiaomi.PushXiaoMiRevicer" android:exported="true">
			<intent-filter>
				<action android:name="com.xiaomi.mipush.RECEIVE_MESSAGE" />
			</intent-filter>
			<intent-filter>
				<action android:name="com.xiaomi.mipush.MESSAGE_ARRIVED" />
			</intent-filter>
			<intent-filter>
				<action android:name="com.xiaomi.mipush.ERROR" />
			</intent-filter>
		</receiver>
		<meta-data android:name="com.ohaola.sdk.push.xiaomi.appid" android:value="${Xiaomi-AppId}" />
		<meta-data android:name="com.ohaola.sdk.push.xiaomi.appkey" android:value="${Xiaomi-AppKey}" />


	</application>
</manifest>